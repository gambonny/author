{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "creator",
  "main": "src/index.ts",
  "compatibility_date": "2025-07-24",
  "observability": {
    "enabled": true
  },
  "vars": {
    "ALLOWED_ORIGINS": "http://localhost:5173,http://localhost:4173",
    "LOG_LEVEL": "info",
    "LOG_FORMAT": "pretty"
  },
  "version_metadata": {
    "binding": "CF_VERSION_METADATA"
  }
}
